# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Resend Configuration
RESEND_API_KEY=your-resend-api-key-here
RESEND_FROM_EMAIL=noreply@autosmart.com

# Vercel Configuration
VERCEL_ENV=production
VERCEL_URL=https://autosmart.vercel.app

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/autosmart
DATABASE_DIRECT_URL=postgresql://user:password@localhost:5432/autosmart

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000
API_SECRET_KEY=your-secret-key-here

# Authentication
JWT_SECRET=your-jwt-secret-key-here
NEXTAUTH_SECRET=your-nextauth-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Third-party Services
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Vehicle API Integration
VEHICLE_API_KEY=your-vehicle-api-key-here
VEHICLE_API_BASE_URL=https://api.vehicle-service.com

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Logging and Monitoring
LOG_LEVEL=info
SENTRY_DSN=your-sentry-dsn-here

# Feature Flags
ENABLE_VOICE_COMMANDS=true
ENABLE_AI_DIAGNOSTICS=true
ENABLE_ROUTE_OPTIMIZATION=true

# Node Environment
NODE_ENV=production